{
  "version": "1.0.0",
  "components": {
    "wb-event-log": {
      "name": "Event Log Component",
      "path": "/components/wb-event-log/",
      "enabled": true,
      "description": "Passive event-driven logging system for Website Builder"
    }
  },
  "server": {
    "port": 8083,
    "mimeTypes": {
      ".js": "application/javascript",
      ".css": "text/css",
      ".json": "application/json",
      ".html": "text/html"
    }
  },
  "themes": {
    "default": "dark",
    "available": ["light", "dark"]
  }
}
