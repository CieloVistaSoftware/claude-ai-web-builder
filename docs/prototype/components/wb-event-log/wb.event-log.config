{
  "name": "wb-event-log",
  "version": "1.0.0",
  "description": "Event logging component for WB Website Builder",
  "config": {
    "maxEvents": 1000,
    "autoScroll": true,
    "filters": {
      "info": true,
      "warning": true,
      "error": true,
      "success": true,
      "debug": true,
      "user": true
    },
    "theme": "dark",
    "position": "top",
    "height": "auto",
    "searchEnabled": true,
    "exportEnabled": true,
    "clearEnabled": true
  },
  "styles": {
    "fontFamily": "monospace",
    "fontSize": "14px",
    "padding": "10px",
    "borderRadius": "4px"
  }
}
