{
  "component": {
    "name": "wb-color-picker",
    "version": "2.0.0",
    "description": "Data-driven color picker component with multiple formats, presets, and eyedropper support",
    "type": "form-component",
    "author": "Website Builder System",
    "dependencies": [],
    "assets": []
  },
  "template": {
    "structure": [
      {
        "tag": "div",
        "attributes": {
          "class": "wb-color-picker-wrapper wb-color-picker--{{variant}} wb-color-picker--{{size}}",
          "data-disabled": { "bind": { "data": "disabled" } },
          "data-format": { "bind": { "data": "format" } }
        },
        "children": [
          {
            "tag": "label",
            "attributes": {
              "class": "wb-color-picker-label",
              "for": "{{id}}"
            },
            "content": "{{label}}",
            "condition": {
              "field": "label",
              "operator": "exists"
            }
          },
          {
            "tag": "div",
            "attributes": {
              "class": "wb-color-picker-trigger",
              "tabindex": "0",
              "role": "button",
              "aria-label": "Open color picker",
              "aria-expanded": "false"
            },
            "children": [
              {
                "tag": "div",
                "attributes": {
                  "class": "wb-color-picker-swatch",
                  "style": "background-color: {{value}}"
                }
              },
              {
                "tag": "input",
                "attributes": {
                  "type": "text",
                  "id": "{{id}}",
                  "class": "wb-color-picker-input",
                  "value": { "bind": { "data": "value" } },
                  "placeholder": "{{placeholder}}",
                  "disabled": { "bind": { "data": "disabled" } },
                  "readonly": { "bind": { "data": "readonly" } }
                }
              },
              {
                "tag": "button",
                "attributes": {
                  "type": "button",
                  "class": "wb-color-picker-eyedropper",
                  "aria-label": "Eyedropper tool",
                  "title": "Pick color from screen"
                },
                "content": "ðŸŽ¯",
                "condition": {
                  "field": "showEyedropper",
                  "operator": "truthy"
                }
              }
            ]
          },
          {
            "tag": "div",
            "attributes": {
              "class": "wb-color-picker-panel",
              "role": "dialog",
              "aria-label": "Color picker"
            },
            "children": [
              {
                "tag": "div",
                "attributes": {
                  "class": "wb-color-picker-canvas"
                },
                "children": [
                  {
                    "tag": "div",
                    "attributes": {
                      "class": "wb-color-picker-saturation",
                      "role": "slider",
                      "aria-label": "Color saturation and brightness"
                    },
                    "children": [
                      {
                        "tag": "div",
                        "attributes": {
                          "class": "wb-color-picker-saturation-pointer"
                        }
                      }
                    ]
                  },
                  {
                    "tag": "div",
                    "attributes": {
                      "class": "wb-color-picker-hue",
                      "role": "slider",
                      "aria-label": "Hue selection"
                    },
                    "children": [
                      {
                        "tag": "div",
                        "attributes": {
                          "class": "wb-color-picker-hue-pointer"
                        }
                      }
                    ]
                  },
                  {
                    "tag": "div",
                    "attributes": {
                      "class": "wb-color-picker-alpha",
                      "role": "slider",
                      "aria-label": "Alpha transparency"
                    },
                    "condition": {
                      "field": "showAlpha",
                      "operator": "truthy"
                    },
                    "children": [
                      {
                        "tag": "div",
                        "attributes": {
                          "class": "wb-color-picker-alpha-pointer"
                        }
                      }
                    ]
                  }
                ]
              },
              {
                "tag": "div",
                "attributes": {
                  "class": "wb-color-picker-controls"
                },
                "children": [
                  {
                    "tag": "div",
                    "attributes": {
                      "class": "wb-color-picker-preview"
                    },
                    "children": [
                      {
                        "tag": "div",
                        "attributes": {
                          "class": "wb-color-picker-preview-current",
                          "style": "background-color: {{value}}",
                          "title": "Current color: {{value}}"
                        }
                      },
                      {
                        "tag": "div",
                        "attributes": {
                          "class": "wb-color-picker-preview-previous",
                          "style": "background-color: {{previousValue}}",
                          "title": "Previous color: {{previousValue}}"
                        },
                        "condition": {
                          "field": "previousValue",
                          "operator": "exists"
                        }
                      }
                    ]
                  },
                  {
                    "tag": "div",
                    "attributes": {
                      "class": "wb-color-picker-inputs"
                    },
                    "children": [
                      {
                        "tag": "div",
                        "attributes": {
                          "class": "wb-color-picker-format-tabs"
                        },
                        "children": [
                          {
                            "tag": "button",
                            "attributes": {
                              "type": "button",
                              "class": "wb-color-picker-format-tab",
                              "data-format": "{{formatOption.value}}",
                              "aria-pressed": "{{formatOption.active}}"
                            },
                            "content": "{{formatOption.label}}",
                            "repeat": {
                              "collection": "formatOptions",
                              "item": "formatOption"
                            }
                          }
                        ]
                      },
                      {
                        "tag": "div",
                        "attributes": {
                          "class": "wb-color-picker-format-inputs"
                        },
                        "children": [
                          {
                            "tag": "input",
                            "attributes": {
                              "type": "text",
                              "class": "wb-color-picker-channel-input",
                              "data-channel": "{{channel.key}}",
                              "value": "{{channel.value}}",
                              "placeholder": "{{channel.placeholder}}",
                              "maxlength": "{{channel.maxLength}}"
                            },
                            "repeat": {
                              "collection": "channels",
                              "item": "channel"
                            }
                          }
                        ]
                      }
                    ]
                  }
                ]
              },
              {
                "tag": "div",
                "attributes": {
                  "class": "wb-color-picker-presets"
                },
                "condition": {
                  "field": "showPresets",
                  "operator": "truthy"
                },
                "children": [
                  {
                    "tag": "div",
                    "attributes": {
                      "class": "wb-color-picker-preset-group"
                    },
                    "children": [
                      {
                        "tag": "h4",
                        "attributes": {
                          "class": "wb-color-picker-preset-title"
                        },
                        "content": "{{presetGroup.title}}",
                        "condition": {
                          "field": "presetGroup.title",
                          "operator": "exists"
                        }
                      },
                      {
                        "tag": "div",
                        "attributes": {
                          "class": "wb-color-picker-preset-colors"
                        },
                        "children": [
                          {
                            "tag": "button",
                            "attributes": {
                              "type": "button",
                              "class": "wb-color-picker-preset-color",
                              "style": "background-color: {{preset.color}}",
                              "data-color": "{{preset.color}}",
                              "title": "{{preset.name}}",
                              "aria-label": "Select {{preset.name}} color"
                            },
                            "repeat": {
                              "collection": "presetGroup.colors",
                              "item": "preset"
                            }
                          }
                        ]
                      }
                    ],
                    "repeat": {
                      "collection": "presetGroups",
                      "item": "presetGroup"
                    }
                  }
                ]
              },
              {
                "tag": "div",
                "attributes": {
                  "class": "wb-color-picker-recent"
                },
                "condition": {
                  "field": "showRecent",
                  "operator": "truthy"
                },
                "children": [
                  {
                    "tag": "h4",
                    "attributes": {
                      "class": "wb-color-picker-recent-title"
                    },
                    "content": "Recent Colors"
                  },
                  {
                    "tag": "div",
                    "attributes": {
                      "class": "wb-color-picker-recent-colors"
                    },
                    "children": [
                      {
                        "tag": "button",
                        "attributes": {
                          "type": "button",
                          "class": "wb-color-picker-recent-color",
                          "style": "background-color: {{recentColor}}",
                          "data-color": "{{recentColor}}",
                          "title": "{{recentColor}}",
                          "aria-label": "Select recent color {{recentColor}}"
                        },
                        "repeat": {
                          "collection": "recentColors",
                          "item": "recentColor"
                        }
                      }
                    ]
                  }
                ]
              },
              {
                "tag": "div",
                "attributes": {
                  "class": "wb-color-picker-actions"
                },
                "children": [
                  {
                    "tag": "button",
                    "attributes": {
                      "type": "button",
                      "class": "wb-color-picker-action wb-color-picker-clear",
                      "aria-label": "Clear color"
                    },
                    "content": "Clear",
                    "condition": {
                      "field": "allowClear",
                      "operator": "truthy"
                    }
                  },
                  {
                    "tag": "button",
                    "attributes": {
                      "type": "button",
                      "class": "wb-color-picker-action wb-color-picker-cancel"
                    },
                    "content": "Cancel"
                  },
                  {
                    "tag": "button",
                    "attributes": {
                      "type": "button",
                      "class": "wb-color-picker-action wb-color-picker-apply"
                    },
                    "content": "Apply"
                  }
                ]
              }
            ]
          },
          {
            "tag": "span",
            "attributes": {
              "class": "wb-color-picker-help-text",
              "id": "{{id}}-help"
            },
            "content": "{{helpText}}",
            "condition": {
              "field": "helpText",
              "operator": "exists"
            }
          }
        ]
      }
    ]
  },
  "styles": {
    "base": {
      "position": "relative",
      "display": "inline-block",
      "fontFamily": "inherit"
    },
    "elements": {
      ".wb-color-picker-label": {
        "display": "block",
        "marginBottom": "0.5rem",
        "fontSize": "0.875rem",
        "fontWeight": "500",
        "color": "var(--text-primary)"
      },
      ".wb-color-picker-trigger": {
        "display": "flex",
        "alignItems": "center",
        "gap": "0.5rem",
        "padding": "0.5rem",
        "backgroundColor": "var(--bg-primary)",
        "border": "1px solid var(--border-color)",
        "borderRadius": "6px",
        "cursor": "pointer",
        "transition": "all 0.2s ease"
      },
      ".wb-color-picker-trigger:hover": {
        "borderColor": "var(--primary)"
      },
      ".wb-color-picker-trigger:focus": {
        "outline": "2px solid var(--primary)",
        "outlineOffset": "2px"
      },
      ".wb-color-picker-swatch": {
        "width": "24px",
        "height": "24px",
        "borderRadius": "4px",
        "border": "1px solid var(--border-color)",
        "flexShrink": "0",
        "backgroundImage": "linear-gradient(45deg, #ccc 25%, transparent 25%), linear-gradient(-45deg, #ccc 25%, transparent 25%), linear-gradient(45deg, transparent 75%, #ccc 75%), linear-gradient(-45deg, transparent 75%, #ccc 75%)",
        "backgroundSize": "8px 8px",
        "backgroundPosition": "0 0, 0 4px, 4px -4px, -4px 0px"
      },
      ".wb-color-picker-input": {
        "flex": "1",
        "border": "none",
        "outline": "none",
        "fontSize": "0.875rem",
        "backgroundColor": "transparent",
        "color": "var(--text-primary)"
      },
      ".wb-color-picker-eyedropper": {
        "border": "none",
        "backgroundColor": "transparent",
        "cursor": "pointer",
        "fontSize": "16px",
        "padding": "2px",
        "borderRadius": "4px",
        "transition": "background-color 0.2s ease"
      },
      ".wb-color-picker-eyedropper:hover": {
        "backgroundColor": "var(--neutral-100, #f8f9fa)"
      },
      ".wb-color-picker-panel": {
        "position": "absolute",
        "top": "100%",
        "left": "0",
        "zIndex": "1000",
        "backgroundColor": "var(--bg-primary)",
        "border": "1px solid var(--border-color)",
        "borderRadius": "8px",
        "padding": "1rem",
        "marginTop": "4px",
        "boxShadow": "0 8px 24px rgba(0, 0, 0, 0.15)",
        "minWidth": "280px",
        "opacity": "0",
        "visibility": "hidden",
        "transform": "translateY(-10px)",
        "transition": "all 0.2s ease"
      },
      ".wb-color-picker-canvas": {
        "marginBottom": "1rem"
      },
      ".wb-color-picker-saturation": {
        "position": "relative",
        "width": "200px",
        "height": "150px",
        "marginBottom": "0.75rem",
        "cursor": "crosshair",
        "background": "linear-gradient(to right, #fff, transparent), linear-gradient(to top, #000, transparent), linear-gradient(to right, #f00, #ff0, #0f0, #0ff, #00f, #f0f, #f00)",
        "borderRadius": "4px"
      },
      ".wb-color-picker-saturation-pointer": {
        "position": "absolute",
        "width": "12px",
        "height": "12px",
        "border": "2px solid white",
        "borderRadius": "50%",
        "boxShadow": "0 0 0 1px rgba(0,0,0,0.3)",
        "transform": "translate(-50%, -50%)",
        "pointerEvents": "none"
      },
      ".wb-color-picker-hue": {
        "position": "relative",
        "width": "200px",
        "height": "12px",
        "marginBottom": "0.75rem",
        "cursor": "pointer",
        "background": "linear-gradient(to right, #f00, #ff0, #0f0, #0ff, #00f, #f0f, #f00)",
        "borderRadius": "6px"
      },
      ".wb-color-picker-hue-pointer": {
        "position": "absolute",
        "width": "16px",
        "height": "16px",
        "border": "2px solid white",
        "borderRadius": "50%",
        "boxShadow": "0 0 0 1px rgba(0,0,0,0.3)",
        "transform": "translate(-50%, -50%)",
        "top": "50%",
        "pointerEvents": "none"
      },
      ".wb-color-picker-alpha": {
        "position": "relative",
        "width": "200px",
        "height": "12px",
        "marginBottom": "0.75rem",
        "cursor": "pointer",
        "borderRadius": "6px",
        "backgroundImage": "linear-gradient(45deg, #ccc 25%, transparent 25%), linear-gradient(-45deg, #ccc 25%, transparent 25%), linear-gradient(45deg, transparent 75%, #ccc 75%), linear-gradient(-45deg, transparent 75%, #ccc 75%)",
        "backgroundSize": "8px 8px"
      },
      ".wb-color-picker-alpha-pointer": {
        "position": "absolute",
        "width": "16px",
        "height": "16px",
        "border": "2px solid white",
        "borderRadius": "50%",
        "boxShadow": "0 0 0 1px rgba(0,0,0,0.3)",
        "transform": "translate(-50%, -50%)",
        "top": "50%",
        "pointerEvents": "none"
      },
      ".wb-color-picker-controls": {
        "display": "flex",
        "gap": "1rem",
        "marginBottom": "1rem"
      },
      ".wb-color-picker-preview": {
        "display": "flex",
        "flexDirection": "column",
        "gap": "4px"
      },
      ".wb-color-picker-preview-current, .wb-color-picker-preview-previous": {
        "width": "40px",
        "height": "20px",
        "border": "1px solid var(--border-color)",
        "borderRadius": "4px",
        "backgroundImage": "linear-gradient(45deg, #ccc 25%, transparent 25%), linear-gradient(-45deg, #ccc 25%, transparent 25%), linear-gradient(45deg, transparent 75%, #ccc 75%), linear-gradient(-45deg, transparent 75%, #ccc 75%)",
        "backgroundSize": "8px 8px"
      },
      ".wb-color-picker-inputs": {
        "flex": "1"
      },
      ".wb-color-picker-format-tabs": {
        "display": "flex",
        "marginBottom": "0.5rem",
        "border": "1px solid var(--border-color)",
        "borderRadius": "4px",
        "overflow": "hidden"
      },
      ".wb-color-picker-format-tab": {
        "flex": "1",
        "padding": "0.25rem 0.5rem",
        "border": "none",
        "backgroundColor": "var(--bg-secondary)",
        "color": "var(--text-secondary)",
        "fontSize": "0.75rem",
        "cursor": "pointer",
        "transition": "all 0.2s ease"
      },
      ".wb-color-picker-format-tab[aria-pressed=\"true\"]": {
        "backgroundColor": "var(--primary)",
        "color": "white"
      },
      ".wb-color-picker-format-inputs": {
        "display": "grid",
        "gridTemplateColumns": "repeat(auto-fit, minmax(50px, 1fr))",
        "gap": "0.25rem"
      },
      ".wb-color-picker-channel-input": {
        "padding": "0.25rem",
        "border": "1px solid var(--border-color)",
        "borderRadius": "4px",
        "fontSize": "0.75rem",
        "textAlign": "center"
      },
      ".wb-color-picker-presets, .wb-color-picker-recent": {
        "marginBottom": "1rem"
      },
      ".wb-color-picker-preset-title, .wb-color-picker-recent-title": {
        "fontSize": "0.75rem",
        "fontWeight": "600",
        "color": "var(--text-secondary)",
        "margin": "0 0 0.5rem",
        "textTransform": "uppercase"
      },
      ".wb-color-picker-preset-colors, .wb-color-picker-recent-colors": {
        "display": "grid",
        "gridTemplateColumns": "repeat(8, 1fr)",
        "gap": "4px"
      },
      ".wb-color-picker-preset-color, .wb-color-picker-recent-color": {
        "width": "24px",
        "height": "24px",
        "border": "1px solid var(--border-color)",
        "borderRadius": "4px",
        "cursor": "pointer",
        "transition": "transform 0.15s ease",
        "backgroundImage": "linear-gradient(45deg, #ccc 25%, transparent 25%), linear-gradient(-45deg, #ccc 25%, transparent 25%), linear-gradient(45deg, transparent 75%, #ccc 75%), linear-gradient(-45deg, transparent 75%, #ccc 75%)",
        "backgroundSize": "8px 8px"
      },
      ".wb-color-picker-preset-color:hover, .wb-color-picker-recent-color:hover": {
        "transform": "scale(1.1)",
        "zIndex": "1"
      },
      ".wb-color-picker-actions": {
        "display": "flex",
        "justifyContent": "flex-end",
        "gap": "0.5rem",
        "paddingTop": "1rem",
        "borderTop": "1px solid var(--border-color)"
      },
      ".wb-color-picker-action": {
        "padding": "0.5rem 1rem",
        "border": "1px solid var(--border-color)",
        "borderRadius": "4px",
        "backgroundColor": "var(--bg-secondary)",
        "color": "var(--text-primary)",
        "fontSize": "0.875rem",
        "cursor": "pointer",
        "transition": "all 0.2s ease"
      },
      ".wb-color-picker-action:hover": {
        "backgroundColor": "var(--neutral-100, #f8f9fa)"
      },
      ".wb-color-picker-apply": {
        "backgroundColor": "var(--primary)",
        "color": "white",
        "borderColor": "var(--primary)"
      },
      ".wb-color-picker-apply:hover": {
        "backgroundColor": "var(--primary-dark)"
      },
      ".wb-color-picker-help-text": {
        "display": "block",
        "marginTop": "0.25rem",
        "fontSize": "0.75rem",
        "color": "var(--text-secondary)"
      }
    },
    "states": {
      "open": {
        ".wb-color-picker-panel": {
          "opacity": "1",
          "visibility": "visible",
          "transform": "translateY(0)"
        },
        ".wb-color-picker-trigger": {
          "borderColor": "var(--primary)"
        }
      },
      "disabled": {
        "opacity": "0.6",
        "pointerEvents": "none",
        ".wb-color-picker-trigger": {
          "cursor": "not-allowed",
          "backgroundColor": "var(--neutral-100, #f8f9fa)"
        }
      }
    },
    "variants": {
      "compact": {
        ".wb-color-picker-panel": {
          "minWidth": "240px"
        },
        ".wb-color-picker-saturation": {
          "width": "160px",
          "height": "120px"
        },
        ".wb-color-picker-hue, .wb-color-picker-alpha": {
          "width": "160px"
        }
      },
      "minimal": {
        ".wb-color-picker-trigger": {
          "border": "none",
          "backgroundColor": "transparent",
          "padding": "0.25rem"
        }
      }
    },
    "sizes": {
      "small": {
        ".wb-color-picker-swatch": {
          "width": "20px",
          "height": "20px"
        },
        ".wb-color-picker-input": {
          "fontSize": "0.75rem"
        }
      },
      "large": {
        ".wb-color-picker-swatch": {
          "width": "32px",
          "height": "32px"
        },
        ".wb-color-picker-input": {
          "fontSize": "1rem"
        }
      }
    },
    "responsive": {
      "(max-width: 640px)": {
        ".wb-color-picker-panel": {
          "position": "fixed",
          "top": "50%",
          "left": "50%",
          "transform": "translate(-50%, -50%)",
          "width": "90vw",
          "maxWidth": "320px"
        }
      }
    }
  },
  "handlers": {
    "click": {
      "selector": ".wb-color-picker-trigger",
      "action": {
        "type": "method",
        "payload": {
          "method": "togglePanel"
        }
      }
    },
    "click": {
      "selector": ".wb-color-picker-preset-color, .wb-color-picker-recent-color",
      "action": {
        "type": "method",
        "payload": {
          "method": "selectColor",
          "args": ["event.target.dataset.color"]
        }
      }
    },
    "click": {
      "selector": ".wb-color-picker-format-tab",
      "action": {
        "type": "method",
        "payload": {
          "method": "setFormat",
          "args": ["event.target.dataset.format"]
        }
      }
    },
    "click": {
      "selector": ".wb-color-picker-apply",
      "action": {
        "type": "method",
        "payload": {
          "method": "applyColor"
        }
      }
    },
    "click": {
      "selector": ".wb-color-picker-cancel",
      "action": {
        "type": "method",
        "payload": {
          "method": "cancelColor"
        }
      }
    },
    "click": {
      "selector": ".wb-color-picker-clear",
      "action": {
        "type": "method",
        "payload": {
          "method": "clearColor"
        }
      }
    },
    "click": {
      "selector": ".wb-color-picker-eyedropper",
      "action": {
        "type": "method",
        "payload": {
          "method": "activateEyedropper"
        }
      }
    },
    "input": {
      "selector": ".wb-color-picker-input",
      "action": {
        "type": "updateData",
        "payload": {
          "value": "event.target.value"
        }
      }
    },
    "input": {
      "selector": ".wb-color-picker-channel-input",
      "action": {
        "type": "method",
        "payload": {
          "method": "updateChannel",
          "args": ["event.target.dataset.channel", "event.target.value"]
        }
      }
    }
  },
  "defaults": {
    "data": {
      "value": "#6366f1",
      "previousValue": "",
      "format": "hex",
      "showAlpha": false,
      "showEyedropper": true,
      "showPresets": true,
      "showRecent": true,
      "allowClear": false,
      "readonly": false,
      "disabled": false,
      "variant": "standard",
      "size": "standard",
      "placeholder": "Enter color...",
      "id": "color-picker-{{timestamp}}",
      "displayValue": "#6366f1",
      "formatOptions": [
        { "value": "hex", "label": "HEX", "active": true },
        { "value": "rgb", "label": "RGB", "active": false },
        { "value": "hsl", "label": "HSL", "active": false }
      ],
