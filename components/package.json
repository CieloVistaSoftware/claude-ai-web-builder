{
  "name": "@claude-ai-web-builder/components",
  "version": "1.0.0",
  "description": "React components for Claude AI Website Builder",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "npm run start:anchor",
    "start:anchor": "vite --open --mode anchor-editor --port 3001 editors/anchor-editor.tsx",
    "start:button": "vite --open --mode button-editor --port 3002 editors/button-editor.tsx",
    "start:image": "vite --open --mode image-editor --port 3003 editors/image-editor.tsx",
    "start:text": "vite --open --mode text-editor --port 3004 editors/text-editor.tsx",
        "start:form": "vite --host 0.0.0.0 --port 3005 --mode form",
    "start:table": "vite --host 0.0.0.0 --port 3006 --mode table",
    "start:video": "vite --host 0.0.0.0 --port 3007 --mode video",
    "start:dashboard": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build",
    "start:table": "vite --open --mode table-editor --port 3006 editors/table-editor.tsx",
    "start:video": "vite --open --mode video-editor --port 3007 editors/video-editor.tsx",
    "start:all": "concurrently \"npm run start:anchor\" \"npm run start:button\" \"npm run start:image\" \"npm run start:text\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7",
    "clsx": "^2.0.0",
    "lucide-react": "^0.290.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "tailwind-merge": "^2.0.0",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^0.34.6",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "storybook": "^7.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "components",
    "website-builder",
    "anchor-editor",
    "ui-components"
  ],
  "author": "CieloVistaSoftware",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CieloVistaSoftware/claude-ai-web-builder.git",
    "directory": "components"
  },
  "bugs": {
    "url": "https://github.com/CieloVistaSoftware/claude-ai-web-builder/issues"
  },
  "homepage": "https://github.com/CieloVistaSoftware/claude-ai-web-builder#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}